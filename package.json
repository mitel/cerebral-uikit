{
  "name": "cerebral-boilerplate",
  "version": "0.1.0",
  "description": "A webpack boilerplate for cerebral applications",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "build-client": "webpack --verbose --colors --display-error-details --config webpack.config.js",
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "stero": "npm run build; cp ./dist/* ./steroids/www/scripts/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/christianalfoni/cerebral-boilerplate.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node"
  ],
  "author": "Christian Alfoni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/christianalfoni/cerebral-boilerplate/issues"
  },
  "homepage": "https://github.com/christianalfoni/cerebral-boilerplate",
  "dependencies": {
    "cerebral": "^0.2.1",
    "jquery": "^2.1.4",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel-core": "^5.1.10",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.4.3",
    "css-loader": "^0.10.1",
    "express": "^4.12.3",
    "file-loader": "^0.8.1",
    "http-proxy": "^1.10.1",
    "imports-loader": "^0.6.3",
    "json-loader": "^0.5.1",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "node-sass": "^3.1.1",
    "react-hot-loader": "^1.2.7",
    "sass-loader": "^1.0.2",
    "script-loader": "^0.6.1",
    "style-loader": "^0.10.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.8.5",
    "webpack-dev-server": "^1.8.0"
  }
}
